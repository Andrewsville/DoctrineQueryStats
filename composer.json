{
	"name": "zenify/doctrine-query-stats",
	"description": "Easy blame of duplicated Doctrine queries for Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.5",
		"kdyby/doctrine": "~2.1",
		"latte/latte": "~2.2",
		"nette/di": "~2.2",
		"tracy/tracy": "~2.2"
	},
	"require-dev": {
		"nette/bootstrap": "~2.2"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineQueryStats\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineQueryStats\\Tests\\": "tests"
		}
	}
}
